<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <Button
        android:id="@+id/cross"
        android:layout_width="62dp"
        android:layout_height="62dp"
        android:layout_marginStart="656dp"
        android:layout_marginBottom="320dp"
        android:backgroundTint="#673AB7"
        android:fontFamily="@font/sigmar_one"
        android:text="X"
        android:textColor="#FFFFFF"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="925dp"
        android:layout_height="723dp"
        android:focusableInTouchMode="true"
        android:rotation="360"
        app:srcCompat="@drawable/bakgrundpop"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="-17dp"
        tools:layout_editor_absoluteY="-156dp" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="510dp"
        android:layout_height="352dp"
        android:layout_marginStart="200dp"
        android:focusableInTouchMode="true"
        android:rotation="360"
        android:tint="#61F3E4E4"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/bakpop"
        tools:ignore="MissingConstraints,UseAppTint"
        tools:layout_editor_absoluteY="29dp" />


</androidx.constraintlayout.widget.ConstraintLayout>